{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@react/*": [
        "src/components/react/*"
      ],
      "@solid/*": [
        "src/components/solid/*"
      ],
      "@astro/*": [
        "src/components/astro/*"
      ],
      "@markdown/*": [
        "src/components/markdown/*"
      ],
      "types/*": [
        "types/*"
      ]
    }
  }
}
